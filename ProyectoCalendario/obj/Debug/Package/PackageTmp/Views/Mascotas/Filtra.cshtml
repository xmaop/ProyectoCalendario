@model IEnumerable<ProyectoCalendario.Models.Mascota>

@{
    ViewBag.Title = "Index";
}
@{
    Layout = "";
}
<h4>Datos Mascota</h4>

<p>

</p>
<table id="tableMascota" class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nombreMascota)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaNacimientoMascota)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.razaMascota)
        </th>
     
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nombreMascota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fechaNacimientoMascota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.razaMascota)
            </td>
            
        </tr>
    }

</table>